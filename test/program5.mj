program MJProgram
	int x; int y;
	int arr[];
	int arrNew[];
	int i, j;
{

	void main() {
		
		arr = new int[5];
		
		i = 0;
		while (i < 5) {
			arr[i] = i + 1;
			i++;
		}
		
		i = 0;
		while (i < 5) {
			print(arr[i]);
			i++;
		} 
			
		arrNew = arr.map(x => 2 * x + 10 * x);
		
		i = 0;
		while (i < 5) {
			print(arrNew[i]);
			i++;
		}
		
	}
	
}