program MJProgram
	int x; int y;
	int matrix[][];
	int i, j;
{

	void main() {
		
		matrix = new int[3][4];
		
		i = 0;
		while(i < 3) {
			j = 0;
			while (j < 4) {
				matrix[i][j] = i * 4 + j;
				j++;
			}
			i++;
		}
		
		i = 0;
		while(i < 3) {
			j = 0;
			while (j < 4) {
				print(matrix[i][j]);
				j++;
			}
			i++;
			print('|');
		}
		
		print(matrix[2][1]);
		
	}
	
}